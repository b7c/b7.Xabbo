<Page x:Class="b7.Xabbo.View.Pages.DebugPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:b7.Xabbo.View.Pages"
      xmlns:viewModel="clr-namespace:b7.Xabbo.ViewModel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      d:DataContext="{d:DesignInstance Type={x:Type viewModel:DebugViewManager}}"
      Title="Debug">
    <Grid>
        <StackPanel Margin="8">
            <Button Content="Test snackbar" Command="{Binding TestSnackbar}" />
        </StackPanel>
    </Grid>
</Page>
